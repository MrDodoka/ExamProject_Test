@page
@using ExamProject_Test.Controllers
@model ExamProject_Test.Pages.CoinPages.RankEightCoinModel
@{
    CoinGeckoController CGC = new();
    var topTenCoins = await CGC.GetTopTenCoins();
    var topEightCoin = await CGC.GetTopNCoin(topTenCoins[7].Id);
}
<head>
    <title>@topEightCoin.Name</title>
</head>
<div>
    <p>Name of the coin: @topEightCoin.Name</p>
    <p>Rank of the coin: @topEightCoin.MarketCapRank</p>
    <p>Price of the coin: @topTenCoins[7].CurrentPrice</p>
</div>