@page
@using ExamProject_Test.Controllers
@model ExamProject_Test.Pages.CoinPages.RankOneCoinModel
@{
    CoinGeckoController CGC = new();
    var topTenCoins = await CGC.GetTopTenCoins();
    var topOneCoin = await CGC.GetTopNCoin(topTenCoins[0].Id);
}
<head>
    <title>@topOneCoin.Name</title>
</head>
<div>
    <p>Name of the coin: @topOneCoin.Name</p>
    <p>Rank of the coin: @topOneCoin.MarketCapRank</p>
    <p>Price of the coin: @topTenCoins[0].CurrentPrice</p>
</div>
