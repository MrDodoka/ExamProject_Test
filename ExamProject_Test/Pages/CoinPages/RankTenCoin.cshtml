@page
@using ExamProject_Test.Controllers
@model ExamProject_Test.Pages.CoinPages.RankTenCoinModel
@{
    CoinGeckoController CGC = new();
    var topTenCoins = await CGC.GetTopTenCoins();
    var topTenCoin = await CGC.GetTopNCoin(topTenCoins[9].Id);
}
<head>
    <title>@topTenCoin.Name</title>
</head>
<div>
    <p>Name of the coin: @topTenCoin.Name</p>
    <p>Rank of the coin: @topTenCoin.MarketCapRank</p>
    <p>Price of the coin: @topTenCoins[9].CurrentPrice</p>
</div>