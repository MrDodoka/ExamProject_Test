@page
@using ExamProject_Test.Controllers
@model ExamProject_Test.Pages.CoinPages.RankNineCoinModel
@{
    CoinGeckoController CGC = new();
    var topTenCoins = await CGC.GetTopTenCoins();
    var topNineCoin = await CGC.GetTopNCoin(topTenCoins[8].Id);
}
<head>
    <title>@topNineCoin.Name</title>
</head>
<div>
    <p>Name of the coin: @topNineCoin.Name</p>
    <p>Rank of the coin: @topNineCoin.MarketCapRank</p>
    <p>Price of the coin: @topTenCoins[8].CurrentPrice</p>
</div>
